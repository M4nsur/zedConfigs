{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Monochrome Zed Retro Hard",
  "author": "m4nsur",
  "themes": [
    {
      "name": "Monochrome Zed (Retro Hard)",
      "appearance": "dark",
      "style": {
        "border": "#00000000",
        "panel.focused_border": "#00000000",
        "pane.focused_border": "#00000000",
        "background": "#0e0f0f",
        "editor.background": "#0e0f0f",
        "panel.background": "#0e0f0f",
        "toolbar.background": "#0e0f0f",
        "status_bar.background": "#0e0f0f",
        "title_bar.background": "#0e0f0f",
        "tab_bar.background": "#0e0f0f",
        "tab.active_background": "#1a1b1b",
        "tab.inactive_background": "#0e0f0f",
        "editor.gutter.background": "#0e0f0f",
        "editor.active_line.background": "#1f2020",
        "editor.active_line_number": "#cfcfcf",
        "editor.line_number": "#3f4040",
        "scrollbar.thumb.background": "#ffffff18",
        "scrollbar.thumb.hover_background": "#ffffff28",
        "text": "#e6e6e6",
        "text.muted": "#5a5a5a",
        "hint": "#5a5a5a",
        "warning": "#ffcc00",
        "warning.background": "#332800",
        "error": "#ff5555",
        "element.active": "#1f2020",
        "element.selected": "#1f2020",
        "terminal.background": "#0e0f0f",
        "terminal.foreground": "#dcdcdc",
        "players": [
          {
            "cursor": "#e6e6e6",
            "selection": "#ffffff22"
          }
        ],
        "syntax": {
          "comment": {
            "color": "#5a5a5a",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#9fbad6"
          },
          "type": {
            "color": "#9fbad6"
          },
          "function": {
            "color": "#b0d0c0"
          },
          "constructor": {
            "color": "#b0d0c0"
          },
          "string": {
            "color": "#a8cfa0"
          },
          "string.escape": {
            "color": "#d7ba7d"
          },
          "number": {
            "color": "#cfa0c0"
          },
          "constant": {
            "color": "#cfa0c0"
          },
          "property": {
            "color": "#b0c4c4"
          },
          "variable": {
            "color": "#e6e6e6"
          },
          "variable.special": {
            "color": "#9fbad6"
          },
          "punctuation": {
            "color": "#d0d0d0"
          },
          "tag": {
            "color": "#9fbad6"
          },
          "link_uri": {
            "color": "#a8cfa0",
            "underline": true
          },
          "emphasis": {
            "font_style": "italic"
          },
          "emphasis.strong": {
            "font_weight": 800
          },
          "diagnostic.hint": {
            "color": "#5a5a5a"
          },
          "diagnostic.warning": {
            "color": "#000000",
            "background": "#ffcc00",
            "font_style": "normal"
          },
          "diagnostic.error": {
            "color": "#ff5555",
            "font_style": "underline"
          }
        }
      }
    }
  ]
}
